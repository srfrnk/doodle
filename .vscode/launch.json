{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "London Police Consumer Boundaries",
            "request": "launch",
            "mainClass": "london_police.App",
            "projectName": "london-police",
            "args": "boundaries"
        },
        {
            "type": "java",
            "name": "London Police Consumer Crimes",
            "request": "launch",
            "mainClass": "london_police.App",
            "projectName": "london-police",
            "args": "crimes"
        },
        {
            "type": "java",
            "name": "Zoopla Consumer Rentals",
            "request": "launch",
            "mainClass": "zoopla.App",
            "projectName": "zoopla",
            "env": {
                "API_KEY_ZOOPLA": ""
            }
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Frontend",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/frontend",
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Api Server",
            "runtimeExecutable": "~/.nvm/versions/node/v11.15.0/bin/node",
            "runtimeArgs": [
                "-r",
                "ts-node/register",
            ],
            "args": [
                "index.ts"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/api"
        }
    ]
}